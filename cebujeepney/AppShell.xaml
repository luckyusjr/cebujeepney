<?xml version="1.0" encoding="UTF-8" ?>
<Shell x:Class="cebujeepney.AppShell"
       xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
       xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
       xmlns:views="clr-namespace:cebujeepney.Views"
       Shell.FlyoutBehavior="Disabled"
       Shell.TitleColor="DarkBlue">

    <ShellContent ContentTemplate="{DataTemplate views:LoginView}"/>
    <ShellContent  ContentTemplate="{DataTemplate views:RegisterView}" Route="RegisterView"/>
    <ShellContent  ContentTemplate="{DataTemplate views:CommuterSettingsView}" Route="CommuterSettingsView"/>
</Shell>
